<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="logImpl" value="SLF4J" />
<!-- 		<setting name="logImpl" value="STDOUT_LOGGING" /> -->
	</settings>
	<typeAliases>
		<typeAlias alias="Integer" type="java.lang.Integer" />
        <typeAlias alias="Long" type="java.lang.Long" />
        <typeAlias alias="HashMap" type="java.util.HashMap" />
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
        <typeAlias alias="ArrayList" type="java.util.ArrayList" />
        <typeAlias alias="LinkedList" type="java.util.LinkedList" />
        
        <typeAlias alias="UserRole" type="com.cn.thinkx.ecom.system.domain.UserRole"/>
		<typeAlias alias="User" type="com.cn.thinkx.ecom.system.domain.User" />
        <typeAlias alias="RoleResource" type="com.cn.thinkx.ecom.system.domain.RoleResource"/>
        <typeAlias alias="Resource" type="com.cn.thinkx.ecom.system.domain.Resource"/>
        <typeAlias alias="Role" type="com.cn.thinkx.ecom.system.domain.Role"/>
        
        <typeAlias alias="Banner"  type="com.cn.thinkx.ecom.banner.domain.Banner"/>
        <typeAlias alias="EshopBanner"  type="com.cn.thinkx.ecom.eshopbanner.domain.EshopBanner"/>
        <typeAlias alias="EshopInf"  type="com.cn.thinkx.ecom.eshop.domain.EshopInf"/>
        <typeAlias alias="Routes"  type="com.cn.thinkx.ecom.routes.domain.Routes"/>
        <typeAlias alias="EshopRoutes"  type="com.cn.thinkx.ecom.eshoproutes.domain.EshopRoutes"/>
        <typeAlias alias="OrderInf"  type="com.cn.thinkx.ecom.order.domain.OrderInf"/>
        <typeAlias alias="OrderDetailUpload"  type="com.cn.thinkx.ecom.order.domain.OrderDetailUpload"/>
        
		<package name="com.cn.thinkx.ecom.basics.member.domain"/>
		<package name="com.cn.thinkx.ecom.basics.goods.domain"/>
		<package name="com.cn.thinkx.ecom.basics.order.domain"/>
	</typeAliases>

</configuration>