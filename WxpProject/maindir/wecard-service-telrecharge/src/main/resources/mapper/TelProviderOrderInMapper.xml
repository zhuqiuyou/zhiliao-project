		WHERE m.data_stat='0' 
		<if test="regOrderId !=null and regOrderId !='' ">
			AND m.REG_ORDER_ID = #{regOrderId}
		</if>
		<if test="channelOrderId !=null and channelOrderId !='' ">
			AND m.CHANNEL_ORDER_ID = #{channelOrderId}
		</if>
		<if test="billId !=null and billId !='' ">
